#include <iostream>
#include <set>
using namespace std;

int main()
{   
    string x ;
    cin >> x ;
    set<char> myset;

  for (int i = 0; i < x.size(); ++i) 
  {
        myset.insert(x[i]);
  }
  
  if(myset.size()%2==0)
    {
        cout << "CHAT WITH HER!";
    }
    else
        cout << "IGNORE HIM!"; 
  return 0;
}
