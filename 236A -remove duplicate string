#include <stdio.h>
#include <string.h>
 
int main()
{   
    char str[100];
    gets(str);
    int length= strlen(str);
    
    for(int i=0; i<length; i++)
    {
        char ch = str[i];
        for(int j=i+1; j<length; )
        {
            if(str[i] == str[j])
            {   
                for(int k=j; k<length; k++)
                {
                    str[k] = str[k+1];
                }
                     length--;
            }
            else 
            {
                 j++;
            }
        } 
    }
   int d=strlen(str);    
    if (d%2==0)
    printf("CHAT WITH HER!");
    else
    printf("IGNORE HIM!");
  return 0;
}
