#include <bits/stdc++.h>
using namespace std;
int main()
{
	int X=0;
	string str,str1="++X",str2="X++",str3="--X",str4="X--";
	int n;
	cin>>n;
	while(n--)
	{
		cin>>str;
		if (str==str1)
		{
			++X;
		}
		else if (str==str3)
		{
			--X;
		}
		else if (str==str2)
		{
			X++;
		}
		else if (str==str4)
		{
			X--;
		}

	}
	cout<<X<<endl;
}
	
