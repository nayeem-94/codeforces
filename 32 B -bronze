#include<bits/stdc++.h>

using namespace std;

int main() 
{
 	string t;
 	cin>>t;
 	int n= t.length();
 	//cout<<t<<n;
 	for (int i = 0; i < n; ++i)
 	{
 		if (t[i]=='.')
 		{
 			cout<<0;
 		}
 		else if (t[i]=='-')
 		{
 			if (t[i+1]=='.')
 			{
 				cout<<1;
 			}
 			else if (t[i+1]=='-')
 			{
 				cout<<2;

 			}
 			i=i+1;
 		}
 		
 	}
}
