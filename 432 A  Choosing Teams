#include<bits/stdc++.h>
using namespace std;


int main()
{	int n;cin>>n;
	int k;cin>>k;
	int arr[n],count=0;

	
	for (int i = 0; i < n; ++i)
	{
		cin>>arr[i];
	}
	sort(arr,arr+n);
	for (int i = 0; i < n; ++i)
	{	
		arr[i]=arr[i]+k;
	
		if (arr[i]<=5)
		{
			count++;
		}
	}
	cout<<count/3;
	
}	 		   	 
